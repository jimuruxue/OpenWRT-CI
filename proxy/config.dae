global {
    log_level: error
    lan_interface: br-lan
    wan_interface: wan
    auto_config_kernel_parameter: true
    allow_insecure: false
    bandwidth_max_tx: '50 mbps'
    bandwidth_max_rx: '200 mbps'
}

subscription {
    airport: '订阅链接'
}

dns {
  upstream {
    googledns: 'tcp+udp://dns.google:53'
    alidns: 'udp://dns.alidns.com:53'
  }
  routing {
    request {
      qname(geosite:category-ads-all) -> reject
      qname(geosite:cn) -> alidns
      fallback: googledns
    }
  }
}

group {
  proxy {
    filter: subtag(airport) && name(keyword: '美国')
    policy: min_moving_avg
  }
  sg {
    filter: subtag(airport) && name(keyword: '新加坡')
    policy: min_moving_avg
  }
}

routing {
  pname(NetworkManager, systemd-resolved, dnsmasq) -> must_direct
  dip(224.0.0.0/3, 'ff00::/8') -> direct
  l4proto(udp) && dport(443) -> block
  domain(suffix: steamcdn-a.akamaihd.net, cm.steampowered.com, steamserver.net, wogg.xxooo.cf) -> direct
  domain(geosite:spotify) -> sg
  dip(geoip:private, geoip:cn) -> direct
  domain(geosite:cn) -> direct
  fallback: proxy
}
